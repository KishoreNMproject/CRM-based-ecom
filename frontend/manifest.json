{
    "name": "Genius Shopppy AI",
    "version": "1.0",
    "manifest_version": 3,
    "description": "A Chrome extension to compare prices on Amazon and analyze deals.",
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
  